{
  "env": {
    "browser": true
  },
  "globals": {
    "$": "readonly",
    "CLDIS": "readonly",
    "CLDN": "readonly",
    "CLD_GLOBAL_TRANSFORMATIONS": "readonly",
    "CLD_ML": "readonly",
    "CLD_VIDEO_PLAYER": "readonly",
    "cloudinary": "readonly",
    "cloudinaryApi": "readonly",
    "cloudinaryGallery": "readonly",
    "cloudinaryGalleryApi": "readonly",
    "jQuery": "readonly",
    "samplePlayer": "readonly",
    "wp": "readonly",
    "wpAjax": "readonly"
  },
  "extends": [
    "plugin:@wordpress/eslint-plugin/recommended"
  ],
  "rules": {
    "camelcase": "warn",
    "no-alert": "off",
    "no-console": "off",
    "no-nested-ternary": "off",
    "jsx-a11y/click-events-have-key-events": "off",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}
